{
    "name": "pro-golf-scores-google-assistant",
    "author": "Alex Zissis <alex@zico.dev>",
    "description": "A Google assistance action for pro-golf scores",
    "version": "0.0.1",
    "dependencies": {
        "@assistant/conversation": "^3.8.1",
        "@nextools/suppress-experimental-warnings": "^0.1.0",
        "body-parser": "^1.19.1",
        "dotenv": "^10.0.0",
        "express": "^4.17.2",
        "i18n-iso-countries": "^7.2.0",
        "node-fetch": "^3.1.0",
        "number-to-words": "^1.2.4"
    },
    "type": "module",
    "devDependencies": {
        "@types/body-parser": "^1.19.2",
        "@types/dotenv": "^8.2.0",
        "@types/express": "^4.17.13",
        "@types/jest": "^27.4.0",
        "@types/node": "^17.0.8",
        "@types/number-to-words": "^1.2.1",
        "esm": "^3.2.25",
        "jest": "^27.4.7",
        "nodemon": "^2.0.15",
        "ts-jest": "^27.1.2",
        "ts-node": "^10.4.0",
        "typescript": "^4.5.4"
    },
    "scripts": {
        "build": "tsc",
        "start:dev": "NODE_ENV=development && nodemon --watch './**/*.ts' --exec 'node -r @nextools/suppress-experimental-warnings --experimental-specifier-resolution=node --loader ts-node/esm' src/app.ts",
        "start:prod": "node -r @nextools/suppress-experimental-warnings dist/app.js",
        "test": "node -r @nextools/suppress-experimental-warnings --experimental-vm-modules node_modules/jest/bin/jest.js"
    }
}
